{
  "manifest_version": 2,
  "name": "Stephen's Hacktastic React Query Devtools Extension",
  "short_name": "SHRQDE",
  "version": "0.0.1",
  "description": "blep",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "bloop",
    "default_popup": "index.html"
  },
  "permissions": ["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
